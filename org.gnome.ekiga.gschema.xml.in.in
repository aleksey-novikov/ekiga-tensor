  <schema gettext-domain="@GETTEXT_PACKAGE@" id="org.gnome.@PACKAGE_NAME@.protocols.h323" path="/org/gnome/@PACKAGE_NAME@/protocols/h323/">
    <key name="listen-port" type="i">
      <default>1720</default>
